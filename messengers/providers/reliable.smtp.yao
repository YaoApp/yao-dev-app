{
  "name": "Reliable SMTP",
  "description": "Reliable SMTP email provider for high availability",
  "connector": "smtp",
  "options": {
    "host": "$ENV.RELIABLE_SMTP_HOST",
    "port": 587,
    "username": "$ENV.RELIABLE_SMTP_USERNAME",
    "password": "$ENV.RELIABLE_SMTP_PASSWORD",
    "from": "$ENV.RELIABLE_SMTP_FROM",
    "use_tls": true
  }
}
