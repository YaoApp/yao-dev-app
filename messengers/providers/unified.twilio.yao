{
  "name": "Main Twilio",
  "description": "Main Twilio provider for SMS/WhatsApp/Email",
  "connector": "twilio",
  "options": {
    "account_sid": "$ENV.TWILIO_ACCOUNT_SID",
    "auth_token": "$ENV.TWILIO_AUTH_TOKEN",
    "from_phone": "$ENV.TWILIO_FROM_PHONE",
    "from_email": "$ENV.TWILIO_FROM_EMAIL",
    "api_sid": "$ENV.TWILIO_API_SID",
    "api_key": "$ENV.TWILIO_API_KEY",
    "sendgrid_api_key": "$ENV.TWILIO_SENDGRID_API_KEY"
  }
}
